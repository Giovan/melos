<!DOCTYPE html>
<html dir="<%= rtl ? 'rtl' : 'ltr' %>">
	<head>
		<link href="/bible-ruby.css" media="screen" rel="stylesheet" type="text/css" />
		<link rel="stylesheet" href="/assets/<%= getAssetPath('main.css') %>" />
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.3.15/slick.css" />
		<script src="https://cdn.ravenjs.com/3.14.0/raven.min.js" crossorigin="anonymous"></script>
		<script type="application/javascript">
			window.__LOCALE__ = <%- JSON.stringify(locale, function(key, val) {
				if (typeof val === 'function') {
					return val + '';
				}
				return val;
			}, 4) %>;
			window.__INITIAL_STATE__ = <%- JSON.stringify(initialState) %>;
			window.__ENV__ = "<%- environment %>";
		</script>
		<meta content="True" name="HandheldFriendly">
		<meta content="320" name="MobileOptimized">
		<meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" name="viewport">
	</head>
	<body>
		<div id="current-ui-view">
			<div id="react-app"><%- appString %></div>
		</div>
		<script src="https://cdn.polyfill.io/v2/polyfill.min.js?features=Intl.~locale.en,Number.isNaN,Promise"></script>
		<script src="/assets/<%= getAssetPath('manifest.js') %>"></script>
		<script src="/assets/<%= getAssetPath('vendor.js') %>"></script>
		<script src="/assets/<%= getAssetPath('main.js') %>"></script>
    <script async defer
      src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAZY4XvuPmG_XKvYCeYyEufzGq0N0wpVI4&libraries=places">
    </script>
    <script type="text/javascript" src="//s7.addthis.com/js/250/addthis_widget.js#pubid=ra-56cdf91ec7a631b2"></script>
    <% if (environment !== 'production') { %>
			<!-- <script type="text/javascript" src="https://jira.lifechurch.tv/s/214836ebd493d3b6d023e0d6a260ed10-T/ok9v4q/73015/01b32b66f1a9832636b5f267a1988181/2.0.23/_/download/batch/com.atlassian.jira.collector.plugin.jira-issue-collector-plugin:issuecollector/com.atlassian.jira.collector.plugin.jira-issue-collector-plugin:issuecollector.js?locale=en-US&collectorId=32b4d264"></script> -->
		<% } %>
	</body>
</html>
