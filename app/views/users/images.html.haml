- @sidebar = false
- @title = @me ? t('users.my images') : t('users.user images', user: @user.name || @user.username)
- content_for :title do
  = @title


.social-feed-wrap{"data-username" => @user.username, "data-kind" => "image"}
  :javascript
    $(document).ready(function(){
      var feed = new Moments.UserFeed();
    });

.profile-blank-slate.slate-images
  - if current_user_is? @user
    %h2
      = t("images.blank slate.heading")
    %p
      = raw t("images.blank slate.description")
  - else
    %h2
      = t("images.none user", username: @user.name)
      