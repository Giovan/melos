+// `Avatars
+//----------------------------------------------------------------------------------------------------
+
+.avatar_small,
+.avatar_medium,
+.avatar_large
+  +border-radius(2px)
+  +box-shadow(rgba(#000, 0.4) 0 1px 1px 0)
+  border: 1px solid #fff
+
+a:hover .avatar_small,
+a:hover .avatar_medium,
+a:hover .avatar_large
+  +glow
+  border-color: $blue
+
+.avatar_small
+  width: 20px
+  height: 20px
+
+.avatar_medium
+  width: 25px
+  height: 25px
+
+.avatar_large
+  width: 45px
+  height: 45px
+
+.avatar_huge
+  +box-shadow(rgba(#000, 0.15) 0 4px 4px 0)
+  background: #fff
+  border: 1px solid #fff
+  padding: 4px
+  width: 130px
+  height: 130px
+
+a:hover .avatar_huge
+  +box-shadow(rgba($blue_light, 0.5) 0 0 5px 0, rgba(#000, 0.15) 0 4px 4px 0)
+  border-color: $blue
\ No newline at end of file
