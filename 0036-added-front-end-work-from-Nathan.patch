+    image: url(../images/shadow.png)
+
+#main .top_section + header
+  +round_top_none
+
+// `Main > Headers
+//----------------------------------------------------------------------------------------------------
+
+.widget header,
+#main header,
+#main footer,
+.header_tabs,
+.top_section
+  @extend .clearfix
